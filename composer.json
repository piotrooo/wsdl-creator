{
  "name": "piotrooo/wsdl-creator",
  "description": "PHP WSDL creator using PHPdoc (annotations, reflections).",
  "minimum-stability": "stable",
  "license": "MIT",
  "keywords": [
    "php",
    "wsdl",
    "generator",
    "creator",
    "annotations",
    "reflections",
    "soap"
  ],
  "authors": [
    {
      "name": "Piotr Olaszewski",
      "email": "piotroo89@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "ext-dom": "*",
    "illuminate/collections": "9.26.1",
    "phpdocumentor/reflection-docblock": "5.3.0",
    "symfony/property-info": "6.0.11"
  },
  "require-dev": {
    "phpunit/phpunit": "9.5.23",
    "friendsofphp/php-cs-fixer": "3.10.0"
  },
  "autoload": {
    "psr-4": {
      "": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "": "tests",
      "Fixtures\\": "tests/Fixtures"
    },
    "files": [
      "tests/AssertPhp/asserts.php"
    ]
  },
  "scripts": {
    "fix-code-style": "php-cs-fixer fix",
    "check-code-style": "php-cs-fixer fix --dry-run"
  }
}
